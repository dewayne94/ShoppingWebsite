<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Patient Forms</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>
<body>

    <div class="navbar navbar-dark @ViewBag.bg navbar-expand-lg ">
        <a href="/Forms/Index" class="navbar-brand">Forms</a>
        <ul class="navbar-nav">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Select Form
                </a>
                <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                    <a class="dropdown-item" href="/Picosure/Create">Picosure</a>
                    <a class="dropdown-item" href="/FotoFacialRF/Create">Foto Facial RF</a>
                    <a class="dropdown-item" href="/LaserHairRemoval/Create">Laser Hair Removal</a>
                    <a class="dropdown-item" href="/FotoFacial/Create">Foto Facial</a>
                    <a class="dropdown-item" href="/GeneralNotes/Create">General Notes</a>
                    <a class="dropdown-item" href="/Cervical/Create">Cervical</a>
                    <a class="dropdown-item" href="/TreatmentLog/Create">Treatment Log</a>
                    <a class="dropdown-item" href="/Intracell/Create">Intracell</a>
                    <a class="dropdown-item" href="/Facialvein/Create">Facial Vein(Male)</a>
                    <a class="dropdown-item" href="/Palomar/Create">Palomar</a>
                    <a class="dropdown-item" href="/Fractora/Create">Factora</a>

                    <a class="dropdown-item" href="/Billing/Create">Billing</a>
                    <a class="dropdown-item" href="/Nonsurgical/Create">Non surgical</a>
                    <a class="dropdown-item" href="/Bodyfx/Create">Body Fx</a>
                    <a class="dropdown-item" href="/Formaface/Create">Forma Face</a>
                </div>
            </li>
        </ul>
        <div class="mx-auto">
            <h2 class="text-white">@ViewBag.FormName</h2>
        </div>
    </div>
    <div class="container-fluid">
        @RenderBody()
    </div>


    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
