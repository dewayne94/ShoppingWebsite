@model PatientForms.Fractora

<style>
    input[type="checkbox"] {
        width: 20px; /*Desired width*/
        height: 20px; /*Desired height*/
    }

    .bg-grey {
        background-color: #808080;
    }
</style>

@{
    ViewBag.bg = "bg-grey";
    ViewBag.FormName = "Fractora / Fractora 3000 Treatment Log";
    ViewBag.Title = "FotoFacial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
@if (TempData["updateMessage"] != null)
{
    <div class="alert alert-success">
        @TempData["updateMessage"]
    </div>
}

<form class="mb-5" action="/Fractora/Update" method="post">
    <div class="row">
        <div class="col-lg-6">
            <div class="form-group">
                <input type="text" name="client_id" class="form-control" placeholder="Client ID" value="@Model.client_id"/>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="form-group">
                <input type="text" name="client_name" class="form-control" placeholder="Client Name" value="@Model.client_name"/>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="form-group">
                <input type="text" name="regions" class="form-control" placeholder="Region(s)" value="@Model.regions"/>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            <label>Pre Photos Taken:</label>
            <div class="form-check form-check-inline">
                <div>
                    @if (Model.pre_photos_taken == 1)
                    {
                        <input type="checkbox" class="form-check-input" name="pre_photos_taken" value="1" checked/>
                    }
                    else
                    {
                        <input type="checkbox" class="form-check-input" name="pre_photos_taken" value="1" />
                    }
                </div>
                <label class="form-check-label">Yes</label>
            </div>
            <div class="form-check form-check-inline">
                <div>
                    @if (Model.pre_photos_taken == 0)
                    {
                        <input type="checkbox" class="form-check-input" name="pre_photos_taken" value="0" checked/>
                    }
                    else
                    {
                        <input type="checkbox" class="form-check-input" name="pre_photos_taken" value="0" />
                    }
                </div>
                <label class="form-check-label">No</label>
            </div>
        </div>
        <div class="col-lg-3">
            <label>Post Photos Taken:</label>
            <div class="form-check form-check-inline">
                <div>
                    @if (Model.post_photos_taken == 1)
                    {
                        <input type="checkbox" class="form-check-input" name="post_photos_taken" value="1" checked/>
                    }
                    else
                    {
                        <input type="checkbox" class="form-check-input" name="post_photos_taken" value="1" />
                    }
                </div>
                <label class="form-check-label">Yes</label>
            </div>
            <div class="form-check form-check-inline">
                <div>
                    @if (Model.post_photos_taken == 0)
                    {
                        <input type="checkbox" class="form-check-input" name="post_photos_taken" value="0" checked/>
                    }
                    else
                    {
                        <input type="checkbox" class="form-check-input" name="post_photos_taken" value="0" />
                    }
                </div>
                <label class="form-check-label">No</label>
            </div>
        </div>
    </div>

